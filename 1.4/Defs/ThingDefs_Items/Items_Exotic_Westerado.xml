  <!--=============== Bison horn ====================-->
  <ThingDef ParentName="ResourceVerbBase_Westerado">
    <defName>BisonHorn_Westerado</defName>
    <label>bison horn</label>
    <description>A bison's horn. It is quite durable and very valuable. While somewhat unwieldy as a melee weapon, it can still be deadly.</description>
    <graphicData>
      <texPath>Things/Item/Special/BisonHorn_a</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <equippedAngleOffset>-20</equippedAngleOffset>
    <statBases>
      <MarketValue>14</MarketValue>
      <MaxHitPoints>110</MaxHitPoints>
      <Mass>0.4</Mass>
      <Flammability>0.9</Flammability>
      <DeteriorationRate>0.5</DeteriorationRate>
    </statBases>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <tools>
      <li>
        <label>point</label>
        <capacities>
          <li>Scratch</li>
          <li>Stab</li>
        </capacities>
        <power>10</power>
        <cooldownTime>2</cooldownTime>
        <chanceFactor>0.5</chanceFactor>
      </li>
      <li>
        <label>base</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>2.6</cooldownTime>
      </li>
    </tools>
    <stuffProps>
      <stuffAdjective>bison horn</stuffAdjective>
      <color>(106,90,48)</color>
      <commonality>0.04</commonality>
    </stuffProps>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
    <thingSetMakerTags>
      <li>AnimalPart</li>
    </thingSetMakerTags>
  </ThingDef>
