<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef>
    <defName>PopPopcorn_Westerado</defName>
    <label>pop corn kernels</label>
    <description>Corn kernels that have been popped. \n\nMakes enough for 5 meals. \n\nGives joy when eaten.</description>
    <jobString>Popping corn.</jobString>
    <workAmount>675</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <requiredGiverWorkType>Cooking</requiredGiverWorkType>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawCorn</li>
          </thingDefs>
        </filter>
        <count>0.50</count>
      </li>
    </ingredients>
    <products>
      <Popcorn_Westerado>5</Popcorn_Westerado>
    </products>
    <skillRequirements>
      <Cooking>3</Cooking>
    </skillRequirements>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawCorn</li>
      </thingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <thingDefs>
        <li>RawCorn</li>
      </thingDefs>
    </defaultIngredientFilter>
    <workSkill>Cooking</workSkill>
    <recipeUsers>
      <li>ElectricStove</li>
      <li>FueledStove</li>
      <li>CampfireLarge</li>
    </recipeUsers>
    <!--<researchPrerequisite>Pemmican</researchPrerequisite>-->
  </RecipeDef>
