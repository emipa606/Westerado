<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <!-- ========== Change tribal label ========== -->
  <Operation Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
      <li Class="PatchOperationReplace">
        <xpath>Defs/FactionDef[defName="TribeRough"]/label</xpath>
        <value>
          <label>barbaric tribe</label>
        </value>
      </li>
    </operations>
  </Operation>
</Patch>