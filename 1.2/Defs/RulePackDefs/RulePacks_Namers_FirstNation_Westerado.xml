<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- First Nation -->
  <RulePackDef>
    <defName>NamerFactionFirstNation_Westerado</defName>
    <include>
      <li>FirstNationWordUtility_Westerado</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[tribalworld] [political_union_firstnation]</li>
        <li>r_name->The [tribalworld] [political_union_firstnation]</li>
        <li>r_name->The [FirstNationColour_Westerado] [TerrainFeature] [political_union_firstnation]</li>
        <li>r_name->[political_union_firstnation] of [WordFirstNation_Westerado]</li>
        <li>r_name->The [FirstNationColour_Westerado] [Animal] [WordTribal]</li>
        <li>r_name->[WordFirstNation_Westerado] [political_union_firstnation]</li>
      </rulesStrings>
      <rulesFiles>
        <li>political_union_firstnation->Words/Nouns/PoliticalUnions_FirstNation_Westerado</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  <RulePackDef>
    <defName>NamerSettlementFirstNation_Westerado</defName>
    <include>
      <li>FirstNationWordUtility_Westerado</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=2)->[WordFirstNation_Westerado]</li>
        <li>r_name->[FirstNationColour_Westerado] [WordFirstNation_Westerado]</li>
        <li>r_name->[FirstNationColour_Westerado] [TerrainFeature]</li>
        <li>r_name->[WordFirstNation_Westerado] [TerrainFeature]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
</Defs>
